<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@color/back_color"
    android:gravity="center">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:gravity="center">

        <TextView
            android:id="@+id/warehouse_name_irir"
            android:layout_width="100.5dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="2dp"
            android:paddingEnd="2dp"
            android:gravity="center"
            android:text="warehouse name"
            android:textSize="12sp"
            android:textStyle="bold" />
        <LinearLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>

        <TextView
            android:id="@+id/warehouse_rack_irir"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="2dp"
            android:paddingEnd="2dp"
            android:gravity="center"
            android:text="warehouse rack"
            android:textSize="12sp"
            android:textStyle="bold" />
        <LinearLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>

        <TextView
            android:id="@+id/warehouse_rcv_qty_irir"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="2dp"
            android:paddingEnd="2dp"
            android:gravity="center"
            android:text="warehouse rcv qty"
            android:textSize="12sp"
            android:textStyle="bold" />
        <LinearLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>

        <TextView
            android:id="@+id/rcv_rate_irir"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="2dp"
            android:paddingEnd="2dp"
            android:gravity="center"
            android:text="rcv rate"
            android:textSize="12sp"
            android:textStyle="bold" />
        <LinearLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>

        <TextView
            android:id="@+id/rcv_amnt_irir"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="2dp"
            android:paddingEnd="2dp"
            android:gravity="center"
            android:text="rcv amnt"
            android:textSize="12sp"
            android:textStyle="bold" />
        <LinearLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>

        <TextView
            android:id="@+id/warehouse_issue_qty_irir"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="2dp"
            android:paddingEnd="2dp"
            android:gravity="center"
            android:text="warehouse issue qty"
            android:textSize="12sp"
            android:textStyle="bold" />
        <LinearLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>

        <TextView
            android:id="@+id/issue_rate_irir"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="2dp"
            android:paddingEnd="2dp"
            android:gravity="center"
            android:text="issue rate"
            android:textSize="12sp"
            android:textStyle="bold" />
        <LinearLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>

        <TextView
            android:id="@+id/issue_amnt_irir"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="5dp"
            android:paddingStart="2dp"
            android:paddingEnd="2dp"
            android:gravity="center"
            android:text="issue amnt"
            android:textSize="12sp"
            android:textStyle="bold" />
        <LinearLayout
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@android:color/darker_gray"/>


</LinearLayout>